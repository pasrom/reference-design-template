&wifi {
	status = "okay";
};

/ {
	aliases {
		golioth-led = &led0;
		sw1 = &button0;
	};

	leds {
		compatible = "gpio-leds";
		led0: led_0 {
			gpios = <&gpio0 25 GPIO_ACTIVE_HIGH>;
			label = "LED 0";
		};
	};

	buttons {
		compatible = "gpio-keys";
		button0: button_0 {
			gpios = < &gpio0 0 (GPIO_PULL_UP | \
			GPIO_ACTIVE_LOW)>;
			label = "PRG Button";
		};
	};

};
